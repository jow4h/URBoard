{
    "manifest_version": 3,
    "name": "URBoard",
    "version": "1.1",
    "description": "URBoard Firefox Extension",
    "chrome_url_overrides": {
        "newtab": "redirect.html"
    },
    "chrome_settings_overrides": {
        "homepage": "redirect.html"
    },
    "permissions": [],
    "icons": {
        "128": "icon128.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "urboard-extension@jow4h.github.io",
            "data_collection_permissions": {
                "required": [
                    "none"
                ]
            }
        }
    }
}